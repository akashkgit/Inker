{
  "manifest_version": 3,
  "name": "Inker",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html"

  },
  "oauth2": {
    "client_id": "994911962715-3gvur88rj0l2mrn746puhnqel2od5lcs.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/userinfo.email","https://www.googleapis.com/auth/documents","https://www.googleapis.com/auth/plus.login"]
  },
  "icons": {
    "16": "inker_16.png",
    "32": "inker_32.png",
    "128": "inker_128.png"
  }, 
  "permissions": [
    "identity","identity.email",
    "contextMenus",
    "storage","tabs"
  ],
  "key":"",
  "web_accessible_resources": [
    {
      "resources": [ "drag.png"],
      "matches": [ "https://*/*" ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "file://*"
      ],
      "js": [
        "cS1.js"
      ],
      "css":["cs1.css"]
      
    }
  ],
  "background": {
    "service_worker": "service.js",
    "type": "module"
  }

}